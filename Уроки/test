²import random



def print_hello(*a):
    b = a
    print('Привет', *b, sep=', ', end='')
    print("!!!")
    #print(f'Вы должны нам {random.randrange(100, 1000000, 50)} рублей!!!')
    print(f'Желаете узнать скины из игры "КУБЫК++" ?')
    aa = input()
    if aa == 'Да' or aa == 'да':
        print(f'original,', 'new,', 'bomber,', 'station.')
    else:
        print('Вы точно не кибергеймер!!!')



while True:
    s = input().split()

    print_hello(*s)